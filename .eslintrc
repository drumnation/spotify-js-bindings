{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:flowtype/recommended",
    "plugin:prettier/recommended",
    "prettier/flowtype"
  ],
  "plugins": [
    "flowtype",
    "flowtype-errors"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "flowtype-errors/show-errors": "error",
    "no-console": 0,
    "no-extend-native": 0,
    "func-names": 0,
    "array-callback-return": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0
  }
}
